{
  "name": "@tokens-studio/sdk",
  "version": "0.0.0-alpha.0",
  "description": "The official SDK for Tokens Studio",
  "main": "./dist/commonjs.cjs",
  "type": "module",
  "module": "./dist/esm.js",
  "jsnext:main": "./dist/esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tokens-studio/token-studio-sdk.git"
  },
  "sideEffects": false,
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "build": "npm run build:js && npm run build:types",
    "build:js": "rollup --bundleConfigAsCjs --config rollup.config.mjs",
    "build:types": "tsc -p tsconfig.json",
    "format": "prettier --write .",
    "format:check": "prettier -c .",
    "test": "jest",
    "docs": "typedoc",
    "reports": "api-extractor run --local",
    "server": "webpack serve",
    "cypress:open": "cypress open"
  },
  "author": "andrew@hyma.io",
  "license": "MIT",
  "dependencies": {
    "@aws-amplify/api": "^5.0.19",
    "@aws-amplify/auth": "^5.1.13",
    "@aws-amplify/core": "^5.1.2",
    "@aws-amplify/pubsub": "^5.1.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "tokens",
    "studio",
    "sdk",
    "management",
    "graphql"
  ],
  "types": "dist/bundle.d.ts",
  "optionalDependencies": {
    "eslint": "^8.31.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "prettier": "^2.8.3"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@microsoft/api-extractor": "^7.34.4",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "cypress": "^12.6.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "jest": "^29.4.0",
    "jest-junit": "^15.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.10.1",
    "rollup-plugin-cleanup-dir": "^1.0.8",
    "rollup-plugin-esbuild": "^5.0.0",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "typedoc": "^0.23.24",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "files": [
    "dist"
  ]
}
