{
  "name": "@tokens-studio/sdk",
  "version": "1.2.0",
  "description": "The official SDK for Tokens Studio",
  "license": "MIT",
  "author": "andrew@hyma.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/tokens-studio/token-studio-sdk.git"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "changeset": "changeset",
    "cypress:open": "cypress open",
    "dev": "tsup --watch",
    "docs": "typedoc",
    "format": "npm run format:eslint && npm run format:prettier",
    "format:eslint": "eslint --ext .tsx,.ts,.js,.html . --fix",
    "format:prettier": "prettier \"**/*.{tsx,ts,js,md,json}\" --write",
    "generate-types": "chokidar 'schema.graphql' -c 'amplify codegen'",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "is-ci || husky install",
    "release": "npm run build && changeset publish",
    "server": "webpack serve",
    "test": "jest"
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@aws-amplify/api": "^5.0.19",
    "@aws-amplify/auth": "^5.1.13",
    "@aws-amplify/core": "^5.1.2",
    "@aws-amplify/pubsub": "^5.1.2",
    "@tokens-studio/types": "^0.3.0"
  },
  "optionalDependencies": {
    "eslint": "^8.31.0",
    "prettier": "^2.8.3"
  },
  "devDependencies": {
    "@aws-amplify/cli": "11.0.3",
    "@changesets/cli": "^2.26.1",
    "@jest/globals": "^29.5.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "chokidar-cli": "^3.0.0",
    "cypress": "^12.6.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^29.4.0",
    "jest-junit": "^15.0.0",
    "lint-staged": "^13.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsup": "^8.1.0",
    "typedoc": "^0.23.24",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "keywords": [
    "graphql",
    "management",
    "sdk",
    "studio",
    "tokens"
  ],
  "publishConfig": {
    "access": "public"
  }
}
