{
  "name": "@tokens-studio/sdk",
  "version": "0.0.0",
  "description": "The official SDK for Tokens Studio",
  "main": "./dist/commonjs.js",
  "module": "./dist/esm.mjs",
  "jsnext:main": "./dist/esm.mjs",
  "repository": "tokens-studio/token-studio-sdk",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "build": "rollup --bundleConfigAsCjs --config rollup.config.mjs",
    "build:types": "tsc -p tsconfig.json",
    "format": "prettier --write .",
    "format:check": "prettier -c .",
    "test": "jest",
    "docs": "typedoc",
    "reports": "api-extractor run --local",
    "server": "webpack serve",
    "cypress:open": "cypress open"
  },
  "author": "andrew@hyma.io",
  "license": "MIT",
  "dependencies": {
    "aws-amplify": "^5.0.9"
  },
  "keywords": [
    "tokens",
    "studio",
    "sdk",
    "management",
    "graphql"
  ],
  "types": "dist/bundle.d.ts",
  "optionalDependencies": {
    "eslint": "^8.31.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "prettier": "^2.8.3"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@microsoft/api-extractor": "^7.34.4",
    "@types/jest": "^29.2.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "cypress": "^12.6.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.4.0",
    "jest-junit": "^15.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.10.1",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "typedoc": "^0.23.24",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "files": [
    "dist"
  ]
}
